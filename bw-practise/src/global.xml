		<param name="server_url" value="http://localhost:8080/bw-practise" />
		<param name="style_url" value="http://localhost:8080/bw-practise/style" />
		<param name="js_url" value="http://localhost:8080/bw-practise/js"/>
		<param name="images_url" value="http://localhost:8080/bw-practise/images" />
	</global>
</global_data>